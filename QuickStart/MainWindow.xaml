<Window x:Class="QuickStart.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="快速启动" Height="200" Width="300" Loaded="Window_Loaded" Closing="Window_Closing">
    <Grid>
        <WrapPanel Height="auto" Width="auto" Margin="0,0,0,0" Name="wraPanel" VerticalAlignment="Top" HorizontalAlignment="Left">
            <Button Name="addButton" Content="add" Margin="10, 10, 0, 0" Height="23" Width="64" Click="addButton_Click"></Button>
                    
        </WrapPanel>
    </Grid>
</Window>
